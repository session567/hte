{
  "manifest_version": 2,
  "name": "HTE",
  "version": "0.1.0",
  "description": "Various enhancements for Hattrick",
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "{9a24700f-1585-42ce-85d1-e2189699151d}",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      }
    }
  },
  "icons": {
    "48": "icons/logo.svg",
    "64": "icons/logo.svg",
    "72": "icons/logo.svg",
    "96": "icons/logo.svg",
    "128": "icons/logo.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.hattrick.org/*"
      ],
      "css": [
        "dist/index.css"
      ],
      "js": [
        "dist/index.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
