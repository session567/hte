{
  "manifest_version": 2,
  "name": "hte",
  "version": "0.1.0",
  "description": "Various enhancements for Hattrick",
  "default_locale": "en",
  "icons": {
    "48": "icons/logo.svg",
    "64": "icons/logo.svg",
    "72": "icons/logo.svg",
    "96": "icons/logo.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.hattrick.org/*"
      ],
      "css": [
        "dist/index.css"
      ],
      "js": [
        "dist/index.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
